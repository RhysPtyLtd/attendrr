</head>

  <body>
  	<%= render 'layouts/header' %>
    <!-- <div class="container"> -->
      <% flash.each do |message_type, message| %> <!-- Displays flash message -->
        <%= content_tag(:div, message, class: "alert flashMessageAlignment alert-#{message_type}") %>
      <% end %>
      <%= yield %>
      <%= render 'layouts/landing_page_footer_script' %>
      <%# render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
    <!-- </div> -->
  </body>
</html>

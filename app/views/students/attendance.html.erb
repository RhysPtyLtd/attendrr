<div class="row text-center center-block">
	<h4><%= link_to(@date_find.strftime("%A, #{@date_find.day.ordinalize} %B %Y"), scheduled_classes_activities_path(search: @date_find)) %></h4>
  <div class="col-sm-4 col-sm-offset-2" id="absent">
    <%= render 'absent_student' %>
  </div>
  <div class="col-sm-4" id="present">
    <%= render 'present_student' %>
  </div>
  
</div>

<script type="text/javascript">
    $(function () {
        $('#datetimepicker4').datetimepicker({
        	format: 'DD/MM/YYYY'
        });
    });
</script>